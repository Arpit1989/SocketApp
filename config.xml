<?xml version="1.0" encoding="UTF-8"?>

<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.arpit.jasmine"
        version   = "1.0">

    <name>Jasmine</name>

    <description>
        Jasmine
    </description>

    <author href="#" email="speak2arpit@gmail.com">
        Arpit Kulshrestha
    </author>

    <preference name="permissions" value="none"/>
    <preference name="fullscreen" value="true" />
  <access origin="*" />
  <feature name="Geolocation">
    <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
  </feature>

    <icon src="Jazmine_72.png" width="72" height="72" />
    <icon src="Jazmine_48.png" width="48" height="48" />
    <icon src="Jazmine_36.png" width="36" height="36" />

    <script src="main-min.js" target="main-min.js" />
    <script src="bootstrap.min.js" target="bootstrap.min.js" />
    <script src="api-min.js" target="api-min.js" />
    <script src="jquery-1.10.2.min.js" target="jquery-1.10.2.min.js" />
    <script src="socket.io-1.0.3-min.js" target="socket.io-1.0.3-min.js" />
    <script src="responsivevoice-min.js" target="responsivevoice-min.js" />

    <asset src="style-min.css" target="style.css" />
    <asset src="bootstrap.min.css" target="bootstrap.min.css" />
    <asset src="wikipedia.png" target="wikipedia.png" />
    <asset src="youtube.png" target="youtube.png" />
    <asset src="Jazmine.png" target="Jazmine.png" />
    <asset src="Jazmine_head.png" target="Jazmine_head.png" />
    <asset src="openweather.png" target="openweather.png" />
    <asset src="comingsoon.png" target="comingsoon.png" />

</widget>
